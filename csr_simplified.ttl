@base <http://purl.org/org/iode/po/voc/cruise-summary-reports>.

@prefix csr: <http://purl.org/org/iode/po/voc/cruise-summary-reports>.
@prefix dct: <http://www.purl.org/dc/terms/>.
@prefix dbo: <http://dbpedia.org/ontology/>.
@prefix foaf: <http://xmlns.com/foaf/spec/#>.
@prefix gl: <http://schema.geolink.org/1.0/base/main#>.
@prefix gsp: <http://www.opengis.net/ont/geosparql#>.
@prefix marineTLO: <http://www.ics.forth.gr/isl/ontology/MarineTLO/>.
@prefix org: <http://www.w3.org/ns/org#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix prov: <http://www.w3.org/ns/prov#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix sdo: <http://schema.org/>.
@prefix vann: <http://purl.org/vocab/vann/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

csr:AssociatedEvent a rdfs:Class.

csr:Cruise a rdfs:Class;
	rdfs:subClassOf gl:Cruise, marineTLO:BC61_Capture_Activity, sdo:Event.

csr:CruiseSummaryReport a rdfs:Class;
	rdfs:subClassOf foaf:Document, gl:Document, prov:Entity;
	gl:documentType [a gl:DocumentType].

csr:Measurement a rdfs:Class;
	rdfs:subClassOf csr:Operation.

csr:Mooring a rdfs:Class;
	rdfs:subClassOf csr:Operation.

csr:Operation a rdfs:Class.

csr:PortCall a rdfs:Class;
	rdfs:subClassOf gl:PortCall, sdo:Event.
	
gl:Cruise a rdfs:Class;
	rdfs:subClassOf csr:Cruise.

gl:PortCall a rdfs:Class;
	rdfs:subClassOf csr:PortCall.

csr:additionalDocumentation rdfs:subPropertyOf rdfs:seeAlso;
    rdfs:domain csr:CruiseSummaryReport;
    rdfs:range xsd:anyURI.

csr:associatedEvent a rdf:Property;
    rdfs:domain csr:Operation;
    rdfs:range csr:AssociatedEvent.
    
csr:atLocation rdfs:subPropertyOf prov:atLocation;
    rdfs:domain csr:AssociatedEvent;
    rdfs:range gsp:wktLiteral.

csr:atPort rdfs:subPropertyOf gl:atPort, prov:atLocation;
    rdfs:domain csr:PortCall;
    rdfs:range xsd:anyUri.

csr:boundingBox rdfs:subPropertyOf dct:spatial;
	rdfs:domain csr:CruiseSummaryReport;
	rdfs:range gsp:wktLiteral.

csr:collateCentre a rdf:Property;
	rdfs:domain csr:CruiseSummaryReport;
	rdfs:range org:Organization.

csr:describesCruise rdfs:subPropertyOf foaf:primaryTopic;
    rdfs:domain csr:CruiseSummaryReport;
    rdfs:range csr:Cruise;
    rdfs:range xsd:anyURI.

csr:describesDataset rdfs:subPropertyOf prov:wasGeneratedBy;
    rdfs:domain csr:CruiseSummaryReport;
    rdfs:range gl:Dataset.

csr:describesOperation rdfs:subPropertyOf prov:wasGeneratedBy;
    rdfs:domain csr:CruiseSummaryReport;
    rdfs:range csr:Operation.

csr:description rdfs:subPropertyOf rdfs:comment;
    rdfs:domain csr:CruiseSummaryReport, csr:Operation;
    rdfs:range rdfs:Literal.

csr:generalOceanArea a rdf:Property;
    rdfs:domain csr:CruiseSummaryReport;
    rdfs:range xsd:anyURI.

csr:hasChiefScientist rdfs:subPropertyOf gl:hasChiefScientist, prov:wasAssociatedWith;
    rdfs:domain csr:Cruise;
    rdfs:range prov:Agent.

csr:hasCoChiefScientist rdfs:subPropertyOf gl:hasCoChiefScientist, prov:wasAssociatedWith;
    rdfs:domain csr:Cruise;
    rdfs:range prov:Agent.
    
csr:hasDOI rdfs:subPropertyOf dct:identifier;
    rdfs:range csr:Cruise;
    rdfs:domain rdfs:Literal, xsd:anyURI.

csr:hasEndPortCall rdfs:subPropertyOf gl:hasEndPortCall, prov:qualifiedEnd;
    rdfs:range csr:Cruise;
    rdfs:domain csr:PortCall.

csr:hasIdentifier rdfs:subPropertyOf rdfs:label;
    rdfs:domain csr:Cruise;
    rdfs:range rdfs:Literal.

csr:hasProgram a rdf:Property;
	rdfs:subPropertyOf prov:wasGeneratedBy;
	rdfs:domain csr:CruiseSummaryReport;
	rdfs:range prov:Activity, dbo:ResearchProject, gl:Program.

csr:hasStartPortCall rdfs:subPropertyOf gl:hasStartPortCall, prov:qualifiedStart;
    rdfs:range csr:Cruise;
    rdfs:domain csr:PortCall.

csr:hasTimeStamp rdfs:subPropertyOf prov:atTime;
    rdfs:domain csr:Operation, csr:PortCall;
    rdfs:range xsd:date, xsd:dateTime.
    
csr:hasTrack rdfs:subPropertyOf gl:hasTrack;
    rdfs:domain csr:Cruise;
    rdfs:range gsp:wktLiteral.

csr:instrumentID a rdf:Property;
	rdfs:subPropertyOf csr:label, rdfs:label;
	rdfs:domain gl:Instrument;
	rdfs:range rdfs:Literal.

csr:instrumentPlatformCode a rdf:Property;
	rdfs:subPropertyOf csr:label, rdfs:label;
	rdfs:domain gl:Instrument;
	rdfs:range rdfs:Literal.

csr:instrumentPlatformDefinition a rdf:Property;
	rdfs:subPropertyOf csr:description, rdfs:comment;
	rdfs:domain gl:Instrument;
	rdfs:range rdfs:Literal.

csr:label a rdf:Property;
	rdfs:subPropertyOf rdfs:label;
	rdfs:range csr:Cruise;
	rdfs:domain rdfs:Literal.

csr:marsdenSquare a rdf:Property;
    rdfs:domain csr:CruiseSummaryReport;
    rdfs:range xsd:anyURI.

csr:numberOfMeasurements a rdf:Property;
    rdfs:domain csr:Measurement;
    rdfs:range xsd:integer.

csr:responsibleLaboratory rdfs:subPropertyOf prov:wasAttributedTo;
    rdfs:domain csr:CruiseSummaryReport;
    rdfs:range foaf:Organization, gl:Organization, org:Organization, prov:Organization.

csr:specificGeographicArea rdfs:subPropertyOf rdfs:comment;
    rdfs:domain csr:CruiseSummaryReport;
    rdfs:range rdfs:Literal.

csr:trackChart rdfs:subPropertyOf rdfs:seeAlso;
    rdfs:domain csr:CruiseSummaryReport;
    rdfs:range xsd:anyURI.

csr:undertakenBy rdfs:subPropertyOf prov:used;
    rdfs:domain csr:Cruise;
    rdfs:range prov:Entity;
    owl:isInverseOf csr:undertakes.

csr:undertakes rdfs:subPropertyOf prov:wasGeneratedBy;
    rdfs:domain prov:Entity;
    rdfs:range csr:Cruise;
    owl:isInverseOf csr:undertakenBy.

gl:atPort rdfs:subPropertyOf csr:atPort.

gl:hasChiefScientist rdfs:subPrpertyOf csr:hasChiefScientist.

gl:hasCoChiefScientist rdfs:subPrpertyOf csr:hasCoChiefScientist.

gl:hasEndPortCall rdfs:subPropertyOf csr:hasEndPortCall.

gl:hasStartPortCall rdfs:subPropertyOf csr:hasStartPortCall.

gl:hasTrack rdfs:subPropertyOf csr:hasTrack.

gl:hasVessel rdfs:subPropertyOf csr:undertakenBy.
